    662           */
    663          
    664          afStatus_t AF_DataRequestSrcRtg( afAddrType_t *dstAddr, endPointDesc_t *srcEP,
    665                                     uint16 cID, uint16 len, uint8 *buf, uint8 *transID,
    666                                     uint8 options, uint8 radius, uint8 relayCnt, uint16* pRelayList )
    667          {
    668            uint8 status;
